<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#c8e2ea"
    android:orientation="vertical"
    tools:context=".Certificar_Wifi" >
    
    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/fondo1"
        android:orientation="vertical" >
        
        <ImageView
            android:layout_width="125dp"
            android:layout_height="55dp"
            android:contentDescription="@string/titulo"
            android:src="@drawable/logo" />
        
        <TextView
            android:id="@+id/textView1"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:gravity="left"
            android:layout_marginLeft="124dp"
            android:layout_marginTop="-48dp"
            android:text="@string/certificacion_tecnica" />
        
        <TextView
            android:id="@+id/cert_titulo"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="124dp"
            android:layout_marginTop="-28dp"
            android:gravity="left"
            android:text="@string/certificar_wifi"
            android:textColor="#258aaf" />
        
	</LinearLayout>
	
    
    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="90dp"
        android:layout_marginBottom="48dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/fondo1"
        android:orientation="vertical" >
        
         <ScrollView
             android:id="@+id/scrollView1"
             android:layout_width="fill_parent"
             android:layout_height="fill_parent"
             android:layout_alignParentTop="true"
             android:layout_toRightOf="@+id/button3" >

             <LinearLayout
                 android:layout_width="match_parent"
                 android:layout_height="match_parent"
                 android:orientation="vertical" >
                 
				    <Button
				        android:text="Certificacion Wifi"
				        android:id="@+id/startBtn"
				        android:layout_width="fill_parent"
				        android:layout_height="wrap_content">
				    </Button>
				    
				    <TextView
				        android:id="@+id/tdown"
				        android:layout_width="fill_parent"
				        android:layout_height="wrap_content"
				        android:text="" />
				    
				    <TextView
				        android:id="@+id/tup"
				        android:layout_width="fill_parent"
				        android:layout_height="wrap_content"
				        android:text="" />
				    
				    
				    <TextView
				        android:id="@+id/test"
				        android:layout_width="fill_parent"
				        android:layout_height="wrap_content"
				        android:text="" />
				    
				    <Button
				        android:text="Consulta WSDL"
				        android:id="@+id/btnWSDL"
				        android:layout_width="fill_parent"
				        android:layout_height="wrap_content">
				    </Button>
				    
				    <TextView
				        android:id="@+id/test_wsdl"
				        android:layout_width="fill_parent"
				        android:layout_height="wrap_content"
				        android:text="Resultado WSDL" />
				    
				    				    
				    
                 
                 
             </LinearLayout>
         </ScrollView>
 
	    
	    
	</LinearLayout>
	
    <RelativeLayout
        android:id="@+id/linearLayout3"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_gravity="center"
        >
        
         <ImageButton
        android:id="@+id/button3"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:contentDescription="@string/volver"
        android:onClick="volver"
        android:src="@android:drawable/ic_media_rew" />

        
        
    </RelativeLayout>
 
</RelativeLayout>  